            "clm__Code__c": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n<meta http-equiv=\"content-type\" content=\"text/html; charset=utf-8\"/>\r\n<title>CT Mobile pharma</title>\r\n<script type=\"text/javascript\" src=\"../JSLibrary/js/app.js\"></script>\r\n\r\n<link href=\"css/style_extra.css\" rel=\"stylesheet\" type=\"text/css\">\r\n\r\n<script src=\"js/jquery-1.7.1.min.js\"></script>\r\n<script src=\"js/jquery-ui.min.js\"></script>\r\n<script src=\"js/jquery.ui.touch-punch.min.js\"></script>\r\n\r\n<script>\r\n\t$(function(){\r\n\t\t$(\".symptom li\").click(function(){\r\n\t\t\t$(this).toggleClass('hard');\r\n\t\t\t\r\n\t\t\tvar countHard = $(\".symptom li.hard\").size(); //alert(countHard);\r\n\t\t\t\r\n\t\t\t$(\".slider_box \").css({'display': 'none'});\r\n\t\t\t\r\n\t\t\tif(countHard<2) {\r\n\t\t\t\t$(\".degree_control\").html('Controlled asthma');\r\n\t\t\t\t$(\".degree-2\").slideUp(300);\r\n\t\t\t\t$(\".degree-3\").slideUp(300);\r\n\t\t\t\t$(\".Stage_jQ\").removeClass('symptom_midd').removeClass('symptom_hard');\r\n\t\t\t\t$(\"#slider-1\").css({'display': 'block'});\r\n\t\t\t} \r\n\t\t\telse if(countHard>2) {\r\n\t\t\t\t$(\".degree_control\").html('Uncontrolled asthma');\r\n\t\t\t\t$(\".degree-2\").slideUp(300, function(){ $(\".degree-3\").slideDown(300); });\r\n\t\t\t\t$(\".Stage_jQ\").removeClass('symptom_midd').addClass('symptom_hard');\r\n\t\t\t\t$(\"#slider-3\").css({'display': 'block'});\r\n\t\t\t} \r\n\t\t\telse {\r\n\t\t\t\t$(\".degree_control\").html('Partially controlled asthma');\r\n\t\t\t\t$(\".degree-3\").slideUp(300, function(){ $(\".degree-2\").slideDown(300); });\r\n\t\t\t\t$(\".Stage_jQ\").removeClass('symptom_hard').addClass('symptom_midd');\r\n\t\t\t\t$(\"#slider-2\").css({'display': 'block'});\r\n\t\t\t};\r\n\t\t});\r\n\t});\r\n</script>\r\n\t<script>\r\n        $(document).ready(function(){\r\n\t\tvar arrSliders = JSON.parse(localStorage.getItem('storage_arrSliders'));\r\n                \r\n                $( \".slider\" ).each(function(iClass){  \r\n                \r\n                    var tooltip = $('<div class=\"tooltip\" id=\"tooltip' + iClass+ '\" />').addClass('txt_border');\r\n                    \r\n                    $(this).slider({\r\n                        animate: false,\r\n                        range: \"min\",\r\n                        value: arrSliders[iClass],\r\n                        min: 1,\r\n                        max: 50,\r\n                        step: 1,\r\n                        create: function (event, ui){\r\n                            var value = $(this).slider(\"option\", \"value\");\r\n                            $(this).find(\".ui-slider-handle\").addClass('shadow').text(value);\r\n                            $(this).find(\".ui-slider-handle\").prepend(tooltip);\r\n                            $('#tooltip'+iClass).text(value);\r\n                        },\r\n                        slide: function( event, ui ) {\r\n                            $(this).find(\".ui-slider-handle\").text(ui.value);\r\n                            $(this).find(\".ui-slider-handle\").prepend(tooltip);\r\n                            $('#tooltip'+iClass).text(ui.value);\r\n                        },\r\n                        start: function( event, ui ) {\r\n                            $('#tooltip'+iClass).css({'display': 'block'});\r\n                        },\r\n                        stop: function( event, ui ) {\r\n                            $('#tooltip'+iClass).css({'display': 'none'});\r\n                            arrSliders[iClass] = ui.value;                                 //console.log(arrSliders[0]);\r\n\r\n                            document.getElementById('input-control').value = arrSliders[0];\r\n                             document.getElementById('input-part').value = arrSliders[1];\r\n                              document.getElementById('input-uncontrol').value = arrSliders[2];\r\n\t\t\t\t\t\t\t\r\n\t\t\t    localStorage.setItem('storage_arrSliders', JSON.stringify(arrSliders)); \r\n                        }\r\n                    });\r\n                });\r\n        });\r\n    </script>\r\n\r\n</head>\r\n\r\n<body>\r\n<div class=\"wrapper gr_grey\">\r\n\r\n<div class=\"Stage_base Control\">\r\n\r\n<h1 class=\"clmeditor-widget-block-content-1\">Asthma control levels</h1>\r\n\r\n<div class=\"Stage_jQ txt_border\">\r\n\r\n<div class=\"degree_control txt_border\">Controlled asthma</div>\r\n<div class=\"degree_control_desc txt_border degree-2\">Any presentation</div>\r\n<div class=\"degree_control_desc txt_border degree-3\">3 or more signs of partially controled asthma</div>\r\n\r\n<ul class=\"symptom\">\r\n<li class=\"txt_border shadow\">\r\n<div class=\"title shadow\">Day symptoms</div>\r\n<div class=\"icon\"></div>\r\n</li>\r\n<li class=\"txt_border shadow\">\r\n<div class=\"title shadow\">Limited activity</div>\r\n<div class=\"icon\"></div>\r\n</li>\r\n<li class=\"txt_border shadow\">\r\n<div class=\"title shadow\">Night symptoms</div>\r\n<div class=\"icon\"></div>\r\n</li>\r\n<li class=\"txt_border shadow\">\r\n<div class=\"title shadow\">First aid medications</div>\r\n<div class=\"icon\"></div>\r\n</li>\r\n<li class=\"txt_border shadow\">\r\n<div class=\"title shadow\">Lung function</div>\r\n<div class=\"icon\"></div>\r\n</li>\r\n</ul>\r\n\r\n<div class=\"slider_title txt_border\">Number of patients</div>\r\n<div class=\"slider_box slider feedback\" id=\"slider-1\">\r\n<div class=\"last_dot\"></div>\r\n</div>\r\n<div class=\"slider_box slider feedback\" id=\"slider-2\">\r\n<div class=\"last_dot\"></div>\r\n</div>\r\n<div class=\"slider_box slider feedback\" id=\"slider-3\">\r\n<div class=\"last_dot\"></div>\r\n</div>\r\n\r\n</div> <!-- Stage_jQ -->\r\n</div> <!-- Stage_base -->\r\n\r\n            <input type=\"hidden\" id=\"input-control\" sobject=\"CTPHARMA__Activity__c\" sfield=\"Controled_asthma_number__c\" value=\"\" class=\"feedback\">\r\n            <input type=\"hidden\" id=\"input-part\" sobject=\"CTPHARMA__Activity__c\" sfield=\"Partialy_Controled_asthma_number__c\" value=\"\" class=\"feedback\">\r\n            <input type=\"hidden\" id=\"input-uncontrol\" sobject=\"CTPHARMA__Activity__c\" sfield=\"Uncontroled_asthma_number__c\" value=\"\" class=\"feedback\">\r\n\r\n            \r\n<footer>\r\n<!-- <a href=\"ALVESCO_Instruction.pdf\" class=\"logo\"></a><!-- -->\r\n<div class=\"referens\">\r\n<strong>GINA Report</strong> 2012\r\n</div>\r\n</footer>\r\n</div> <!-- wrapper -->\r\n\r\n\r\n</body>\r\n</html>",
